@model IEnumerable<Veterinaria.Web.Models.Consult>

@{
    ViewBag.Title = "Index";
}

<h2>Reservación</h2>

<p>
    @Html.ActionLink("Crear uno nuevo", "Create")
</p>
<table class="table-striped table-info">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.ConsultDate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Description)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ConsultType)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.ConsultDate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Description)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ConsultType)
        </td>
        <td>
            @Html.ActionLink("Editar", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Detalles", "Details", new { id=item.Id }) |
            @Html.ActionLink("Eliminar", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
